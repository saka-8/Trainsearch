<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>ProjectNew</display-name>
  <welcome-file-list>
    <welcome-file>password.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>Raildetail</servlet-name>
  <servlet-class>railway.Raildetail</servlet-class>
  <init-param>
  <param-name>username</param-name>
  <param-value>root</param-value>
  </init-param>
  <init-param>
  <param-name>pwd</param-name>
  <param-value>scott</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>Raildetail</servlet-name>
  <url-pattern>/gs</url-pattern>
  </servlet-mapping>
  <context-param>
  <param-name>url</param-name>
  <param-value>jdbc:mysql://localhost:3306</param-value>
  </context-param>
  <context-param>
  <param-name>fqcn</param-name>
  <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
</web-app>
  
	  
 